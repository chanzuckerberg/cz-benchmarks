    "version": "0.2.0",
    "configurations": [
        {
            "name": "Example Model Runner (mlflow)",
            "type": "debugpy",
            "request": "launch",
            "module": "src.czibench.runners.example_model_runner",
            "console": "integratedTerminal",
            "args": [
                "--model-runtime-type", "mlflow",
                "--model-url", "${workspaceFolder}/model-serving/runtimes/mlflow/scvi/runtime",
